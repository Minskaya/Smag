<Page x:Class="CoconFx.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:CoconFx"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      Title="Page 2"
      d:DataContext="{d:DesignInstance local:Page2ViewModel}"
      d:DesignHeight="300"
      d:DesignWidth="300"
      mc:Ignorable="d">

    <StackPanel Style="{StaticResource RootPanel}">
        <TextBlock>Page 2</TextBlock>
        <Button Command="{Binding NavigateBackCommand}"
                Content="Back" />
    </StackPanel>
</Page>